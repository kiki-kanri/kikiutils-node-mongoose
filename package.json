{
  "name": "@kikiutils/mongoose",
  "version": "1.0.0-rc.0",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/kikiutils-node-mongoose.git"
  },
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc-multi",
    "bumplog": "changelogen --bump"
  },
  "dependencies": {
    "decimal.js": "^10.4.3",
    "lodash-es": "^4.17.21",
    "mongoose": "^8.4.0",
    "mongoose-aggregate-paginate-v2": "^1.0.7",
    "mongoose-paginate-v2": "^1.8.1"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.1",
    "@kikiutils/tsconfigs": "^2.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.12",
    "changelogen": "^0.5.5",
    "tsc-multi": "^1.1.0",
    "typescript": "^5.4.5"
  },
  "keywords": [
    "mongooose",
    "mongooose-plugins",
    "mongooose-types",
    "mongooose-utils"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./*": {
      "default": "./*.mjs",
      "types": "./*.d.ts"
    },
    "./plugins/*": {
      "default": "./plugins/*.mjs",
      "types": "./plugins/*.d.ts"
    },
    "./types": {
      "types": "./types/index.d.ts"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
