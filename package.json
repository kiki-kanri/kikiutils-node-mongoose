{
  "name": "@kikiutils/mongoose",
  "version": "1.2.0",
  "description": "A Mongoose plugin for enhanced JSON normalization and common schema creation, with built-in support for pagination and automatic Decimal128 conversion.",
  "license": "MIT",
  "author": "kiki-kanri",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiki-kanri/kikiutils-node-mongoose.git"
  },
  "scripts": {
    "build": "ts-project-builder ./src/constants.ts ./src/utils.ts --clean --preserve-modules",
    "bumplog": "changelogen --bump"
  },
  "dependencies": {
    "decimal.js": "^10.4.3",
    "lodash": "^4.17.21",
    "mongoose": "^8.5.1",
    "mongoose-aggregate-paginate-v2": "^1.1.1",
    "mongoose-paginate-v2": "^1.8.2"
  },
  "devDependencies": {
    "@kikiutils/prettierrc": "^1.3.2",
    "@kikiutils/tsconfigs": "^3.0.1",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.12",
    "changelogen": "^0.5.5",
    "ts-project-builder": "^3.0.0"
  },
  "keywords": [
    "Decimal128",
    "json",
    "mongo",
    "mongoose",
    "normalization",
    "pagination",
    "plugin",
    "schema"
  ],
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./*": {
      "import": "./*.mjs",
      "require": "./*.cjs",
      "types": "./*.d.ts"
    },
    "./_connections": {
      "import": null,
      "require": null,
      "types": null
    },
    "./plugins/*": {
      "import": "./plugins/*.mjs",
      "require": "./plugins/*.cjs",
      "types": "./plugins/*.d.ts"
    },
    "./types": {
      "types": "./types/index.d.ts"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    }
  },
  "prettier": "@kikiutils/prettierrc"
}
